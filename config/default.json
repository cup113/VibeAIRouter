{
  "env": "development",
  "port": 3001,
  
  "providers": {
    "siliconflow": {
      "name": "SiliconFlow",
      "baseUrl": "https://api.siliconflow.cn/v1",
      "enabled": true,
      "priority": 1,
      "timeout": 30000,
      "maxRetries": 2
    },
    "gitee": {
      "name": "Gitee AI",
      "baseUrl": "https://ai.gitee.com/v1",
      "enabled": true,
      "priority": 2,
      "timeout": 30000,
      "maxRetries": 2
    }
  },
  
  "models": {
    "default": "Qwen/Qwen3-8B",
    
    "siliconflow": [
      "deepseek-ai/DeepSeek-R1-0528-Qwen3-8B",
      "Qwen/Qwen3-8B",
      "THUDM/GLM-4.1V-9B-Thinking",
      "internlm/internlm2_5-7b-chat"
    ],
    
    "gitee": [
      "Lingshu-32B",
      "DeepSeek-R1-Distill-Qwen-14B",
      "GLM-4.6V-Flash",
      "Qwen3-4B"
    ],
    
    "mapping": {
      "deepseek-ai/DeepSeek-R1-0528-Qwen3-8B": "siliconflow",
      "Qwen/Qwen3-8B": "siliconflow",
      "THUDM/GLM-4.1V-9B-Thinking": "siliconflow",
      "internlm/internlm2_5-7b-chat": "siliconflow",
      "Lingshu-32B": "gitee",
      "DeepSeek-R1-Distill-Qwen-14B": "gitee",
      "GLM-4.6V-Flash": "gitee",
      "Qwen3-4B": "gitee"
    }
  },
  
  "timeouts": {
    "firstToken": 20000,
    "total": 160000,
    "connect": 10000,
    "read": 30000
  },
  
  "rateLimiting": {
    "ip": {
      "windowMs": 60000,
      "max": 30,
      "message": "Too many requests from this IP, please try again later."
    },
    "global": {
      "windowMs": 60000,
      "max": 100,
      "message": "Too many requests, please try again later."
    }
  },
  
  "logging": {
    "level": "info",
    "logRequests": true,
    "excludeFields": ["messages", "api_key", "password"]
  },
  
  "security": {
    "maxTokens": 20000,
    "maxMessages": 100,
    "allowedOrigins": ["*"]
  }
}